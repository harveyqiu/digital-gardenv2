{
  "name": "starter-blog",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "next dev",
    "dev": "INIT_CWD=$PWD next dev",
    "build": "INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node -r esbuild-register ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts"
  },
  "dependencies": {
    "@next/bundle-analyzer": "13.0.3",
    "@next/font": "13.0.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "contentlayer": "0.2.9",
    "esbuild": "0.14.38",
    "github-slugger": "^1.5.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.0",
    "mdast-util-to-string": "^3.2.0",
    "mdx-bundler": "^9.2.1",
    "next": "13.0.3",
    "next-contentlayer": "0.2.9",
    "next-themes": "^0.2.1",
    "pliny": "0.0.9",
    "postcss": "^8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^0.4.0",
    "rehype-katex": "^6.0.3",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-wiki-link-plus": "^1.1.1",
    "tailwindcss": "^3.3.2",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "esbuild-register": "3.3.3",
    "eslint": "^8.39.0",
    "eslint-config-next": "13.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "globby": "11.0.3",
    "husky": "^8.0.3",
    "js-yaml": "4.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
